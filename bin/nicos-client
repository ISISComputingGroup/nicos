#!/usr/bin/env python

import os
import sys

if os.name != 'posix':
    print('The NICOS text client is not available on this platform.')
    sys.exit(1)

from os import path

sys.path.insert(0, path.dirname(path.dirname(path.realpath(__file__))))

from nicos.clients.cli import main

sys.exit(main(sys.argv))
