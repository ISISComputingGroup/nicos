#!/usr/bin/env python

import argparse
import sys

from os import path

sys.path.insert(0, path.dirname(path.dirname(path.realpath(__file__))))

from nicos.services.web import WebSession

parser = argparse.ArgumentParser()
parser.add_argument('setup', nargs='?', default='startup',
                    help="start setup [default='startup']")
opts = parser.parse_args()

WebSession.run(opts.setup)
